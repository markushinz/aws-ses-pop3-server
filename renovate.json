{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":enablePreCommit",
    "docker:pinDigests"
  ],
  "kubernetes": {
    "managerFilePatterns": [
      "/\\.ya?ml$/"
    ]
  },
  "bumpVersion": "patch",
  "rebaseWhen": "behind-base-branch",
  "postUpdateOptions": [
    "gomodTidy"
  ]
}
